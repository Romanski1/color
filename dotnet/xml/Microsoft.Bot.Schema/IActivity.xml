      <MemberSignature Language="C#" Value="public dynamic ChannelData { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance object ChannelData" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Bot.Schema.IActivity.ChannelData" />
      <MemberSignature Language="VB.NET" Value="Public Property ChannelData As Object" />
